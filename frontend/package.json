{
    "name": "frontend",
    "version": "1.0.0",
    "type": "module",
    "engines": {
        "node": "18.x.x",
        "npm": ">=9.x.x"
    },
    "scripts": {
        "lint:css": "npx stylelint \"src/**/*.css\"",
        "lint:js": "npx eslint \"src/**/*.{ts,tsx}\"",
        "lint:type": "npx tsc --noEmit",
        "lint": "npm run lint:type && npm run lint:js",
        "start:dev": "vite",
        "build": "tsc -p tsconfig.build.json && vite build",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build -o build/storybook"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "7.6.17",
        "@storybook/addon-interactions": "7.6.17",
        "@storybook/addon-links": "7.6.17",
        "@storybook/addon-onboarding": "1.0.11",
        "@storybook/blocks": "7.6.17",
        "@storybook/builder-vite": "7.6.17",
        "@storybook/react": "7.6.17",
        "@storybook/react-vite": "7.6.17",
        "@types/react": "18.2.55",
        "@types/react-dom": "18.2.19",
        "@vitejs/plugin-react": "4.2.1",
        "autoprefixer": "10.4.17",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "postcss": "8.4.35",
        "prettier-plugin-tailwindcss": "0.5.11",
        "storybook": "7.6.17",
        "tailwindcss": "3.4.1",
        "vite": "5.0.12",
        "vite-plugin-pwa": "0.18.1",
        "vite-plugin-svgr": "4.2.0"
    },
    "dependencies": {
        "@heroicons/react": "2.1.1",
        "@hookform/resolvers": "3.3.4",
        "@reduxjs/toolkit": "2.1.0",
        "@stripe/react-stripe-js": "2.5.0",
        "@stripe/stripe-js": "3.0.4",
        "chart.js": "4.4.1",
        "clsx": "2.1.0",
        "date-fns": "3.3.1",
        "framer-motion": "11.0.8",
        "jwt-decode": "4.0.0",
        "react": "18.2.0",
        "react-chartjs-2": "5.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.50.1",
        "react-multi-date-picker": "4.4.1",
        "react-redux": "9.1.0",
        "react-router-dom": "6.22.0",
        "react-select": "5.8.0",
        "react-toastify": "10.0.4",
        "shared": "*"
    }
}
